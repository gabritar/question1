preco_fabrica = float(input("Digite o preco de fábrica: "))

if preco_fabrica <= 12000:
	print("O valor do carro é R$%.2f." % (preco_fabrica + (preco_fabrica * 0.05)))
elif preco_fabrica <= 25000:
	print("O valor do carro é R$%.2f." % (preco_fabrica + (preco_fabrica * 0.10) + (preco_fabrica * 0.15)))
else:
	print("O valor do carro é R$%.2f." % (preco_fabrica + (preco_fabrica * 0.15) + (preco_fabrica * 0.20)))
